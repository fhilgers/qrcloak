---
source: qrcloak-core/src/payload/format.rs
expression: schema
---
{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Payload",
  "anyOf": [
    {
      "$ref": "#/definitions/CompletePayload"
    },
    {
      "$ref": "#/definitions/PartialPayload"
    }
  ],
  "definitions": {
    "CompletePayload": {
      "type": "object",
      "required": [
        "data"
      ],
      "properties": {
        "data": {
          "type": "string",
          "format": "base45",
          "pattern": "^[0-9A-Z\\s\\$%\\*\\+\\-\\.\\/:]*$"
        },
        "payload_metadata": {
          "$ref": "#/definitions/PayloadMetadata"
        }
      }
    },
    "CompressionSpec": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "gzip"
              ]
            }
          }
        }
      ]
    },
    "EncryptionSpec": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "age_key"
              ]
            }
          }
        },
        {
          "type": "object",
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "age_password"
              ]
            }
          }
        },
        {
          "type": "object",
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "no_encryption"
              ]
            }
          }
        }
      ]
    },
    "IndexMetadata": {
      "type": "object",
      "required": [
        "id",
        "index",
        "size"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "index": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "size": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        }
      }
    },
    "PartialPayload": {
      "type": "object",
      "required": [
        "data",
        "index_metadata"
      ],
      "properties": {
        "data": {
          "$ref": "#/definitions/PartialPayloadData"
        },
        "index_metadata": {
          "$ref": "#/definitions/IndexMetadata"
        }
      }
    },
    "PartialPayloadData": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "data",
            "type"
          ],
          "properties": {
            "data": {
              "type": "string",
              "format": "base45",
              "pattern": "^[0-9A-Z\\s\\$%\\*\\+\\-\\.\\/:]*$"
            },
            "payload_metadata": {
              "$ref": "#/definitions/PayloadMetadata"
            },
            "type": {
              "type": "string",
              "enum": [
                "head"
              ]
            }
          }
        },
        {
          "type": "object",
          "required": [
            "data",
            "type"
          ],
          "properties": {
            "data": {
              "type": "string",
              "format": "base45",
              "pattern": "^[0-9A-Z\\s\\$%\\*\\+\\-\\.\\/:]*$"
            },
            "type": {
              "type": "string",
              "enum": [
                "tail"
              ]
            }
          }
        }
      ]
    },
    "PayloadMetadata": {
      "type": "object",
      "properties": {
        "compression": {
          "anyOf": [
            {
              "$ref": "#/definitions/CompressionSpec"
            },
            {
              "type": "null"
            }
          ]
        },
        "encryption": {
          "anyOf": [
            {
              "$ref": "#/definitions/EncryptionSpec"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    }
  }
}
